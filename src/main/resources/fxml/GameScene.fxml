<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="GamePane" prefHeight="600.0" prefWidth="1000.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.unideb.inf.freecell.game.GameSceneController">
    <stylesheets>
        <URL value="@/styles/gamescene.css" />
    </stylesheets>
   <children>
       
      <MenuBar fx:id="gameMenu" layoutX="-2.0" prefHeight="30.0" prefWidth="1000.0">
        <menus>
          <Menu mnemonicParsing="false" text="Game">
            <items>
              <MenuItem fx:id="newGameMenuItem" mnemonicParsing="false" onAction="#onNewGameMenuAction" text="New Game" />
              <MenuItem fx:id="saveGameMenuItem" mnemonicParsing="false" onAction="#onSaveGameMenuAction" text="Save Game" />
              <MenuItem fx:id="loadGameMenuItem" mnemonicParsing="false" onAction="#onLoadGameMenuAction" text="Load Game" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      
   </children>
</AnchorPane>
